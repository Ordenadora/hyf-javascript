console.log(y="Nada");
y="Nada";
console.log('the value of my string will be: Nada');
console.log(y);
/* 5. How do you round the number 7.25, to the nearest integer?*/
var z=7.25;
console.log(z);
var a=Math.round(7.25);
console.log(a);
var l=Math.max(z,a);
console.log(l);
